       !((N0 != mynode & Dir_InvSet_N0) | (N1 != mynode & Dir_InvSet_N1)
        | (N2 != mynode & Dir_InvSet_N2) | (N3 != mynode & Dir_InvSet_N3)) &
      Dir_Pending & Dir_InvSet_N2 : FALSE;
--  rule "NI_InvAck"
