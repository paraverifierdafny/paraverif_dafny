  next(InvMsg_Cmd):=
    case
--  rule "NI_InvAck"
      mynode != Home & mynode = N0 &
      InvMsg_Cmd = inv_invack &
      Dir_Pending & Dir_InvSet_N0 : inv_none;
--  rule "NI_InvAck"
      mynode != Home & mynode = N1 &
      InvMsg_Cmd = inv_invack &
      Dir_Pending & Dir_InvSet_N1 : inv_none;
